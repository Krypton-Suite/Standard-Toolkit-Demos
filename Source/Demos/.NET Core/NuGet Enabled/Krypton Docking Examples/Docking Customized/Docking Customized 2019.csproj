<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFrameworks>net35;net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;net5.0-windows</TargetFrameworks>
    <OutputType>WinExe</OutputType>
    <RootNamespace>DockingCustomized</RootNamespace>
    <AssemblyName>Docking Customized</AssemblyName>
    <ApplicationIcon>Krypton.ico</ApplicationIcon>
    <OldToolsVersion>3.5</OldToolsVersion>
    <PublishUrl>publish\</PublishUrl>
    <Install>true</Install>
    <InstallFrom>Disk</InstallFrom>
    <UpdateEnabled>false</UpdateEnabled>
    <UpdateMode>Foreground</UpdateMode>
    <UpdateInterval>7</UpdateInterval>
    <UpdateIntervalUnits>Days</UpdateIntervalUnits>
    <UpdatePeriodically>false</UpdatePeriodically>
    <UpdateRequired>false</UpdateRequired>
    <MapFileExtensions>true</MapFileExtensions>
    <ApplicationRevision>0</ApplicationRevision>
    <ApplicationVersion>1.0.0.%2a</ApplicationVersion>
    <IsWebBootstrapper>false</IsWebBootstrapper>
    <UseApplicationTrust>false</UseApplicationTrust>
    <AssemblyVersion>19.3.603.9</AssemblyVersion>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="ContentDocument.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="ContentInput.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="ContentPropertyGrid.cs">
      <SubType>UserControl</SubType>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <Content Include="Krypton.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Krypton.Docking" Version="5.550.2011" />
    <PackageReference Include="Krypton.Navigator" Version="5.550.2011" />
    <PackageReference Include="Krypton.Ribbon" Version="5.550.2011" />
    <PackageReference Include="Krypton.Toolkit" Version="5.550.2011" />
    <PackageReference Include="Krypton.Workspace" Version="5.550.2011" />
  </ItemGroup>
  <PropertyGroup>
    <OutputPath>..\..\..\..\..\Binaries\Krypton Demos\$(Configuration)\</OutputPath>
  </PropertyGroup>
</Project>