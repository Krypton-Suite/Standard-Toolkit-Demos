<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
    <PropertyGroup>
        <TargetFrameworks>net35;net40;net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;netcoreapp5.0</TargetFrameworks>
        <OutputType>WinExe</OutputType>
        <RootNamespace>PaletteUpgradeTool</RootNamespace>
        <AssemblyName>Palette Upgrade Tool</AssemblyName>
        <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
        <UseWindowsForms>true</UseWindowsForms>
    </PropertyGroup>
    <PropertyGroup>
        <ApplicationIcon>Resources\Krypton.ico</ApplicationIcon>
    </PropertyGroup>
    <PropertyGroup>
        <OutputPath>..\..\..\..\..\Binaries\Krypton Demos\$(Configuration)\</OutputPath>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="Krypton.Toolkit" Version="5.500.2003" />
    </ItemGroup>
</Project>